{
  "name": "tkl-electron",
  "version": "1.0.8",
  "description": "听客来",
  "main": "index.js",
  "author": "南京创世德意",
  "MIN_WINDOW_WIDTH": 1218,
  "MIN_WINDOW_HEIGHT": 630,
  "CompanyName": "南京创世德意",
  "FileDescription": "听客来",
  "OriginalFilename": "听客来.exe",
  "ProductName": "听客来",
  "InternalName": "听客来",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "all": "electron-builder -mw",
    "release": "electron-builder"
  },
  "build": {
    "productName": "听客来",
    "appId": "tkl__demo",
    "copyright": "xxxx",
    "mac": {
      "category": "public.app-category.productivity",
      "icon": "res/app.icns",
      "type": "distribution"
    },
    "dmg": {
      "artifactName": "rishiqing-mac-${productName}-${version}.${ext}",
      "title": "日事清 ${version}",
      "contents": [
        {
          "x": 30,
          "y": 190
        },
        {
          "x": 150,
          "y": 190,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        {
          "target": "nsis"
        }
      ],
      "icon": "assets/tkl.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "assets/tkl.ico",
      "uninstallerIcon": "assets/tkl.ico",
      "installerHeaderIcon": "assets/tkl.ico",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "听客来"
    }
  },
  "publish": [
    "github"
  ],
  "keywords": [],
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "cross-env": "^6.0.3",
    "electron": "^7.1.3",
    "electron-builder": "^21.2.0",
    "electron-updater": "^4.2.0"
  }
}
